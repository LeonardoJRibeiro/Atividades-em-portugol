algoritmo "exercício 3"
var
   comp, larg, alt, vol: real
inicio
      escreva ("Digite o valor do comprimento: ")
       leia (comp)
      escreva ("Digite o valor da largura: ")
       leia (larg)
      escreva ("Digite o valor da altura: ")
       leia (alt)
            vol <- (comp*larg*alt)
       escreva ("O volume da caixa é: ",vol,"Um³.")
fimalgoritmo