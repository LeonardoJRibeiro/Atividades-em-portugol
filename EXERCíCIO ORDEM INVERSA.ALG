algoritmo "exercício 7"
var
   numa, numb: real
inicio
      escreva ("Digite o primeiro número: ")
       leia (numa)
      escreva ("Digite o segundo número: ")
       leia (numb)
      escreva ("Os valores na ordem inversa são: ")
      escreval
      escreva (numb)
      escreval
      escreva (numa)
fimalgoritmo