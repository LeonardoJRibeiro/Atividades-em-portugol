algoritmo "exercício 4"
var
   val, quad_, cub: real
inicio
      escreva ("Digite o valor: ")
       leia (val)
            quad_ <- (val*val)
            cub <- (val*val*val)
       escreva ("O quadrado do valor é: ",quad_,".")
       escreval
       escreva ("O cubo do valor é: ",cub,".")
fimalgoritmo