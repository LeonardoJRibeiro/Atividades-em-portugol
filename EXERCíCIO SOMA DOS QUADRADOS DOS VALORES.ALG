algoritmo "exercício 5"
var
   a, b, quad_a, quad_b, soma_quads: real
inicio
      escreva ("Digite o primeiro valor: ")
       leia (a)
      escreva ("Digite o segundo valor: ")
       leia (b)
            quad_a <- (a*a)
            quad_b <- (b*b)
            soma_quads <- (quad_a+quad_b)
       escreva ("A soma dos quadrados dos valores é: ",soma_quads,".")
fimalgoritmo